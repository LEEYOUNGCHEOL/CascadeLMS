<div id="form_area">
<h2>Editing project team for <%= @course.title %></h2>
<p><%= link_to 'Cancel', :course => @course, :controller => '/instructor/teams' %></p>

<%= start_form_tag :action => 'update', :id => @team %>
  <%= render :partial => 'form' %>
<div class="submit">
  <%= submit_tag 'Edit' %>
</div>
<%= end_form_tag %>

</div>

<p><%= link_to 'Cancel', :course => @course, :controller => '/instructor/teams' %></p>