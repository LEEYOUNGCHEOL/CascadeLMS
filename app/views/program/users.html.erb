<h2>Program Managers/Auditors for '<%= @program.title %>'</h2>

<div class="btn-toolbar">
 <div class="btn-group">
  <%= link_to 'Done Editing', {:action => nil, :id => @program}, {:class => 'btn btn-primary'} %>
 </div>
</div>

<h3>Program Managers</h3>

<div id="managers_list">
 <%= render :partial => 'userlist', :locals => {:users => @managers, :utype => 'manager', :program => @program} %>
</div>

<hr/>
<h3>Program Auditors</h3>

<div id="auditors_list">
 <%= render :partial => 'userlist', :locals => {:users => @auditors, :utype => 'auditor', :program => @program} %>
</div>
