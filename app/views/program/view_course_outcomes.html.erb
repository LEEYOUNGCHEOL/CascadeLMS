<p>
 <%= link_to "Back", :controller => '/program', :action => 'courses', :id => @program, :term => @course.term %>	
</p>

<div class="SectionHeader"><div class="Full">
<h2>Programs for <%= @course.title %> (<%= @course.term.semester %>)</h2>
</div></div>

    <p>This course is part of the following programs:
	<ul>
	  <% for cprogram in @course.programs %>
	    <li id="program_<%=cprogram.id%>"><%=h cprogram.title %></span></li>
	  <% end %>
	</ul>
	<br/>
	</p>
	
<div class="SectionHeader"><div class="Full">
<h2>Course Outcomes for <%= @course.title %> (<%= @course.term.semester %>)</h2>
</div></div>

<%= render :partial => 'course_outcomes_part' %>