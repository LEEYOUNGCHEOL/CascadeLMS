  [4;35;1mStyleCheck Create (0.2ms)[0m   [0mINSERT INTO `style_checks` (`name`, `example`, `bias`, `description`) VALUES('MisleadingVariableName', 'public&nbsp;class&nbsp;Foo&nbsp;{<br/>&nbsp;&nbsp;&nbsp;&nbsp;private&nbsp;int&nbsp;m_foo;&nbsp;//&nbsp;OK<br/>&nbsp;&nbsp;&nbsp;&nbsp;public&nbsp;void&nbsp;bar(String&nbsp;m_baz)&nbsp;{&nbsp;&nbsp;//&nbsp;Bad<br/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;int&nbsp;m_boz&nbsp;=&nbsp;42;&nbsp;//&nbsp;Bad<br/>&nbsp;&nbsp;&nbsp;&nbsp;}<br/>&nbsp;&nbsp;}', 1, 'Detects when a non-field has a name starting with \'m_\'. This usually<br/>indicates a field and thus is confusing.')[0m
