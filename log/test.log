  [4;35;1mStyleCheck Create (0.1ms)[0m   [0mINSERT INTO `style_checks` (`name`, `example`, `bias`, `description`) VALUES('SimplifyBooleanReturns', 'public&nbsp;class&nbsp;Foo&nbsp;{<br/>&nbsp;&nbsp;private&nbsp;int&nbsp;bar&nbsp;=2;<br/>&nbsp;&nbsp;public&nbsp;boolean&nbsp;isBarEqualsTo(int&nbsp;x)&nbsp;{<br/>&nbsp;&nbsp;&nbsp;&nbsp;//&nbsp;this&nbsp;bit&nbsp;of&nbsp;code<br/>&nbsp;&nbsp;&nbsp;&nbsp;if&nbsp;(bar&nbsp;==&nbsp;x)&nbsp;{<br/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;return&nbsp;true;<br/>&nbsp;&nbsp;&nbsp;&nbsp;}&nbsp;else&nbsp;{<br/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;return&nbsp;false;<br/>&nbsp;&nbsp;&nbsp;&nbsp;}<br/>&nbsp;&nbsp;&nbsp;&nbsp;//&nbsp;can&nbsp;be&nbsp;replaced&nbsp;with&nbsp;a&nbsp;simple<br/>&nbsp;&nbsp;&nbsp;&nbsp;//&nbsp;return&nbsp;bar&nbsp;==&nbsp;x;<br/>&nbsp;&nbsp;}<br/>}', 1, 'Avoid unnecessary if..then..else statements when returning a boolean')[0m
